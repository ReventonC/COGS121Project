<!DOCTYPE HTML>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" media="screen" href="/css/style.min.css" />
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/js-cookie.js"></script>
    <script src="/js/login.js"></script>
</head>

<body>
    <header>
        <div id="brandBox">
            <img onclick="window.location='kitchen'" id="logo" src="/img/logo.png" alt="logo" style="width:50px;height:31px;"/>
            <p id="appName">
                <span style="color: #43E97C">pan</span><span style="color: #3AF7CA">try</span>
            </p>
        </div>
    </header>

    <div class="content">
        <div id="loginBox">
            <img onclick="window.location='kitchen'" id="logo" src="/img/logo.png" alt="logo" style="width:150px;height:93px;"/>
            <h1>Login</h1>
            <div class="input">
                <p>Username</p>
                <input id='user' type="text" name="Username">
                <br>
            </div>
            <div class="input">
                <p>Password</p>
                <input id='pass' type="text" name="Password">
                <br>
            </div>
            <button id="submit_btn">Submit</button>
            <button id="signup_btn" onclick="window.location=''">Sign Up</button>
        </div>
    </div>

    <footer>

    </footer>
</body>

</html>
